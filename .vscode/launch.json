  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Launch GenerateAst",
      "request": "launch",
      "mainClass": "fr.jellycat.GenerateAst",
      "projectName": "tool",
      "args": ["jeevee/src/main/java/fr/jellycat"]
    },
    {
      "type": "java",
      "name": "Launch Jeevee REPL",
      "request": "launch",
      "mainClass": "fr.jellycat.Jeevee",
      "projectName": "jeevee"
    },
    {
      "type": "java",
      "name": "Launch Jeevee Script",
      "request": "launch",
      "mainClass": "fr.jellycat.Jeevee",
      "projectName": "jeevee",
      "args": ["test.eve"]
    }
  ]
}
